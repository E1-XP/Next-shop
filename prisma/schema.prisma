generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id        Int      @id @default(autoincrement())
  productId String
  name      String
  brand     String
  price     Int
  oldPrice  Int
  rating    Int
  color     String
  images    String[]
}
