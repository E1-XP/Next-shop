generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id        Int      @id @default(autoincrement())
  modelId   String
  productId String
  name      String
  brand     String
  price     Int
  oldPrice  Int
  rating    Int
  color     String
  xs        Int
  sm        Int
  md        Int
  lg        Int
  xl        Int
  xxl       Int
  images    String[]
}
